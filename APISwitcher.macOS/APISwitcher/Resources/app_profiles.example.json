    "name": "示例配置 - 自定义端点",
    "balanceApi": "https://custom-api.example.com/balance",
    "authMode": "header",
    "authKey": "Authorization",
    "apiKey": "Bearer YOUR_API_KEY",
    "balanceJsonPath": "$.data.balance",
    "subscriptionApi": "https://custom-api.example.com/subscription",
    "subscriptionJsonPath": "$.data",
    "settings": {
      "claude": {
        "apiKey": "sk-custom-xxx",
        "model": "claude-sonnet-4.5",
        "baseURL": "https://custom-api.example.com/v1"
      },
      "mcpServers": {
        "filesystem": {
          "command": "npx",
          "args": [
            "-y",
            "@modelcontextprotocol/server-filesystem",
            "/Users/username/Documents"
          ]
        }
      }
    }
  }
